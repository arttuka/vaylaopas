{
  "name": "vaylaopas",
  "version": "0.1.0",
  "license": "AGPL-3.0-only",
  "engines": {
    "node": "12.18.1"
  },
  "private": true,
  "dependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "@material-ui/core": "v4.11.0",
    "@material-ui/icons": "^4.9.1",
    "express": "^4.17.1",
    "mapbox-gl": "^1.11.1",
    "notistack": "^0.9.17",
    "pg": "^8.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "4.12.21",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3"
  },
  "scripts": {
    "watch:server": "BABEL_ENV=server NODE_ENV=development nodemon -e js,ts,tsx --exec 'babel-node -x \".js\",\".ts\",\".tsx\"' src/server/server.ts",
    "watch:client": "BABEL_ENV=server NODE_ENV=development webpack-dev-server -d --colors --progress --config webpack.dev.babel.js",
    "watch": "concurrently -k -n server,client 'npm run watch:server' 'npm run watch:client'",
    "lint": "eslint . --ext .js,.ts,.tsx --max-warnings=0 && prettier --check src/**/*.{js,ts,tsx}",
    "test": "BABEL_ENV=server jest",
    "test:watch": "BABEL_ENV=server jest --watch",
    "convert": "NODE_ENV=development ts-node ./src/convert/convert.ts",
    "build": "npm run build:babel && npm run webpack:prod",
    "build:babel": "BABEL_ENV=server NODE_ENV=production babel src --extensions '.js','.ts','.tsx' --copy-files --out-dir dist",
    "webpack:prod": "webpack --require @babel/register --display-error-details --config webpack.prod.js",
    "start": "NODE_ENV=production node dist/server/server.js",
    "clean": "rimraf dist/*"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.5",
    "@types/mapbox-gl": "^1.11.1",
    "@types/node": "^14.0.24",
    "@types/pg": "^7.14.4",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/webpack": "^4.41.21",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^26.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^3.0.7",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^5.0.9"
  }
}
