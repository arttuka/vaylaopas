{
  "name": "vaylaopas",
  "version": "0.1.0",
  "license": "AGPL-3.0-only",
  "private": true,
  "sideEffects": false,
  "dependencies": {
    "@material-ui/core": "v4.9.14",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "mapbox-gl": "^1.10.1",
    "pg": "^8.2.1",
    "pg-native": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "scripts": {
    "server:watch": "NODE_ENV=development ts-node ./src/server/index.ts",
    "typecheck:watch": "tsc --noEmit --watch",
    "watch": "concurrently -n ts,build -c green,blue \"npm run typecheck:watch\" \"npm run server:watch\"",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0 && prettier --check src/**/*.{ts,tsx}",
    "test": "jest",
    "test:watch": "jest --watch",
    "convert": "NODE_ENV=development ts-node ./src/convert/convert.ts",
    "build": "NODE_ENV=production webpack && tsc src/server/index.ts --outdir dist --esModuleInterop",
    "start": "NODE_ENV=production node dist/index.js",
    "clean": "rimraf dist/*"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.2",
    "@types/mapbox-gl": "^1.10.0",
    "@types/node": "^14.0.1",
    "@types/pg": "^7.14.3",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/webpack": "^4.41.13",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "html-loader": "^1.1.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-hot-server-middleware": "^0.6.0",
    "webpack-node-externals": "^1.7.2"
  }
}
