{
  "name": "vaylaopas",
  "version": "0.1.0",
  "license": "AGPL-3.0-only",
  "engines": {
    "node": "22"
  },
  "private": true,
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@dnd-kit/core": "^6.3.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^6.4.7",
    "@mui/material": "^6.4.7",
    "@redux-devtools/extension": "^3.2.2",
    "clsx": "^2.1.1",
    "express": "^4.17.1",
    "kysely": "^0.27.4",
    "maplibre-gl": "^4.2.0",
    "notistack": "^3.0.1",
    "pg": "^8.7.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "uuid": "^11.1.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.3"
  },
  "scripts": {
    "watch:server": "ts-node-dev --watch src/server,src/common src/server/server.ts",
    "watch:client": "BABEL_ENV=server NODE_ENV=development webpack serve --color --config webpack.dev.babel.js",
    "watch:client:parcel": "parcel ./src/client/index.html --port 8081",
    "watch:typecheck": "tsc --noEmit --watch --preserveWatchOutput --pretty",
    "watch": "concurrently -k -n server,client,typecheck -c magenta,green,blue 'npm run watch:server' 'npm run watch:client' 'npm run watch:typecheck'",
    "watch:parcel": "concurrently -k -n server,client,typecheck 'npm run watch:server' 'npm run watch:client:parcel' 'npm run watch:typecheck'",
    "lint": "tsc --noEmit && eslint \"src/**/*.{js,ts,tsx}\" --max-warnings=0 && prettier --check \"src/**/*.{js,ts,tsx}\"",
    "typecheck": "tsc --noEmit --pretty",
    "test": "jest",
    "test:watch": "jest --watch",
    "convert": "NODE_ENV=development ts-node ./src/convert/convert.ts",
    "build": "parcel build",
    "start": "NODE_ENV=production node --enable-source-maps dist/server/server.js",
    "clean": "rimraf dist/* .parcel-cache"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/register": "^7.15.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
    "@types/express": "^4.17.13",
    "@types/geojson": "^7946.0.8",
    "@types/jest": "^29.5.12",
    "@types/mapbox__point-geometry": "^0.1.2",
    "@types/node": "^22.13.9",
    "@types/parcel-bundler": "^1.12.4",
    "@types/pg": "^8.6.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/react-redux": "^7.1.18",
    "@types/uuid": "^10.0.0",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "parcel": "^2.12.0",
    "prettier": "^3.2.5",
    "react-refresh": "^0.16.0",
    "rimraf": "^6.0.1",
    "source-map-loader": "^5.0.0",
    "terser-webpack-plugin": "^5.2.4",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.23.0",
    "webpack": "^5.53.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "targets": {
    "client": {
      "source": "src/client/index.html",
      "context": "browser",
      "publicUrl": "/js"
    },
    "server": {
      "source": "src/server/server.ts",
      "context": "node"
    }
  }
}
