{
  "name": "vaylaopas",
  "version": "0.1.0",
  "license": "AGPL-3.0-only",
  "engines": {
    "node": "14.15.0"
  },
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.1.0",
    "@emotion/styled": "^10.0.27",
    "@hot-loader/react-dom": "^17.0.0",
    "@material-ui/core": "^5.0.0-alpha.14",
    "@material-ui/icons": "^v5.0.0-alpha.14",
    "clsx": "^1.1.1",
    "express": "^4.17.1",
    "mapbox-gl": "^1.12.0",
    "notistack": "^1.0.1",
    "pg": "^8.4.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-loader": "4.13.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3"
  },
  "scripts": {
    "watch:server": "BABEL_ENV=server NODE_ENV=development nodemon -e js,ts,tsx --exec 'babel-node -x \".js\",\".ts\",\".tsx\"' src/server/server.ts",
    "watch:client": "BABEL_ENV=server NODE_ENV=development webpack-dev-server -d --colors --progress --config webpack.dev.babel.js",
    "watch": "concurrently -k -n server,client 'npm run watch:server' 'npm run watch:client'",
    "lint": "tsc --noEmit && eslint \"src/**/*.{js,ts,tsx}\" --max-warnings=0 && prettier --check \"src/**/*.{js,ts,tsx}\"",
    "test": "BABEL_ENV=server jest",
    "test:watch": "BABEL_ENV=server jest --watch",
    "convert": "NODE_ENV=development ts-node ./src/convert/convert.ts",
    "build": "npm run build:babel && npm run webpack:prod",
    "build:babel": "BABEL_ENV=server NODE_ENV=production babel src --extensions '.js','.ts','.tsx' --copy-files --out-dir dist",
    "webpack:prod": "webpack --require @babel/register --display-error-details --config webpack.prod.js",
    "start": "NODE_ENV=production node dist/server/server.js",
    "clean": "rimraf dist/*"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/mapbox-gl": "^1.12.5",
    "@types/node": "^14.14.6",
    "@types/pg": "^7.14.5",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-redux": "^7.1.9",
    "@types/webpack": "^4.41.24",
    "@types/webpack-env": "^1.15.3",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^4.2.3",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.10.3",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^5.3.0"
  }
}
