  },
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^5.0.0-alpha.29",
    "@material-ui/icons": "^5.0.0-alpha.28",
    "clsx": "^1.1.1",
    "express": "^4.17.1",
    "mapbox-gl": "^1.13.1",
    "notistack": "^1.0.5",
    "pg": "^8.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "^1.1.3"
  },
  "scripts": {
    "watch:server": "BABEL_ENV=server NODE_ENV=development nodemon -e js,ts -w src/server -w src/common --exec 'babel-node -x \".js\",\".ts\"' src/server/server.ts",
    "watch:client": "BABEL_ENV=server NODE_ENV=development webpack serve --color --config webpack.dev.babel.js",
    "watch:typecheck": "tsc --noEmit --watch --preserveWatchOutput --pretty",
    "watch": "concurrently -k -n server,client,typecheck 'npm run watch:server' 'npm run watch:client' 'npm run watch:typecheck'",
    "lint": "tsc --noEmit && eslint \"src/**/*.{js,ts,tsx}\" --max-warnings=0 && prettier --check \"src/**/*.{js,ts,tsx}\"",
    "test": "BABEL_ENV=server jest",
    "test:watch": "BABEL_ENV=server jest --watch",
    "convert": "NODE_ENV=development ts-node ./src/convert/convert.ts",
    "build": "webpack --config webpack.prod.js",
    "start": "NODE_ENV=production node --enable-source-maps dist/server/server.js",
    "clean": "rimraf dist/*"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.14",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/mapbox-gl": "^1.13.0",
    "@types/node": "^14.14.37",
    "@types/pg": "^7.14.11",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-redux": "^7.1.16",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.0.1",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.2",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "react-refresh": "^0.10.0",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.1.1",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.31.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^2.5.2"
  }
}
