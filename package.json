{
  "name": "vaylaopas",
  "version": "0.1.0",
  "license": "AGPL-3.0-only",
  "engines": {
    "node": "12.18.1"
  },
  "private": true,
  "dependencies": {
    "@material-ui/core": "v4.9.14",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "mapbox-gl": "^1.10.1",
    "pg": "^8.2.1",
    "pg-native": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "4.12.20"
  },
  "scripts": {
    "watch:server": "BABEL_ENV=server NODE_ENV=development nodemon -e js,ts,tsx --exec 'babel-node -x \".js\",\".ts\",\".tsx\"' src/server/server.ts",
    "watch:client": "BABEL_ENV=server NODE_ENV=development webpack-dev-server -d --colors --progress --config webpack.dev.babel.js",
    "watch": "concurrently -k -n server,client 'npm run watch:server' 'npm run watch:client'",
    "lint": "eslint . --ext .js,.ts,.tsx --max-warnings=0 && prettier --check src/**/*.{js,ts,tsx}",
    "test": "BABEL_ENV=server jest",
    "test:watch": "BABEL_ENV=server jest --watch",
    "convert": "NODE_ENV=development ts-node ./src/convert/convert.ts",
    "build": "npm run build:server && npm run build:client && npm run webpack:prod",
    "build:client": "BABEL_ENV=server NODE_ENV=production babel src/client --copy-files --out-dir dist/client",
    "build:server": "BABEL_ENV=server NODE_ENV=production babel src/server --copy-files --out-dir dist/server",
    "webpack:prod": "webpack --require @babel/register --display-error-details --config webpack.prod.js",
    "start": "NODE_ENV=production node dist/server/server.js",
    "clean": "rimraf dist/*"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/register": "^7.9.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.2",
    "@types/mapbox-gl": "^1.10.0",
    "@types/node": "^14.0.1",
    "@types/pg": "^7.14.3",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/webpack": "^4.41.13",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^2.3.5",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
