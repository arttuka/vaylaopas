{
  "name": "vaylaopas",
  "version": "0.1.0",
  "license": "AGPL-3.0-only",
  "private": true,
  "sideEffects": false,
  "dependencies": {
    "@material-ui/core": "^4.1.3",
    "@material-ui/icons": "^4.2.1",
    "axios": "^0.19.0",
    "express": "^4.17.1",
    "mapbox-gl": "^1.0.0",
    "pg": "^7.11.0",
    "pg-native": "^3.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.3.2"
  },
  "scripts": {
    "server:watch": "NODE_ENV=development ts-node ./src/server/index.ts",
    "typecheck:watch": "tsc --noEmit --watch",
    "watch": "concurrently -n ts,build -c green,blue \"npm run typecheck:watch\" \"npm run server:watch\"",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0 && prettier --check src/**/*.{ts,tsx}",
    "test": "jest",
    "test:watch": "jest --watch",
    "convert": "NODE_ENV=development ts-node ./src/convert/convert.ts",
    "build": "NODE_ENV=production webpack && tsc src/server/index.ts --outdir dist --esModuleInterop",
    "start": "NODE_ENV=production node dist/index.js",
    "clean": "rimraf dist/*"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.15",
    "@types/mapbox-gl": "^0.51.9",
    "@types/node": "^12.0.10",
    "@types/pg": "^7.4.14",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/styled-components": "4.1.8",
    "@types/webpack": "^4.4.34",
    "@types/webpack-env": "^1.13.9",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "concurrently": "^4.1.1",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "html-loader": "^0.5.5",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-hot-server-middleware": "^0.6.0",
    "webpack-node-externals": "^1.7.2"
  }
}
