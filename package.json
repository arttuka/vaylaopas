{
  "name": "vaylaopas",
  "version": "0.1.0",
  "license": "AGPL-3.0-only",
  "private": true,
  "sideEffects": false,
  "dependencies": {
    "@material-ui/core": "v4.9.1",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "mapbox-gl": "^1.7.0",
    "pg": "^7.18.1",
    "pg-native": "^3.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.0.1"
  },
  "scripts": {
    "server:watch": "NODE_ENV=development ts-node ./src/server/index.ts",
    "typecheck:watch": "tsc --noEmit --watch",
    "watch": "concurrently -n ts,build -c green,blue \"npm run typecheck:watch\" \"npm run server:watch\"",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0 && prettier --check src/**/*.{ts,tsx}",
    "test": "jest",
    "test:watch": "jest --watch",
    "convert": "NODE_ENV=development ts-node ./src/convert/convert.ts",
    "build": "NODE_ENV=production webpack && tsc src/server/index.ts --outdir dist --esModuleInterop",
    "start": "NODE_ENV=production node dist/index.js",
    "clean": "rimraf dist/*"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.2",
    "@types/mapbox-gl": "^1.7.0",
    "@types/node": "^13.7.0",
    "@types/pg": "^7.14.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "4.4.2",
    "@types/webpack": "^4.41.5",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "html-loader": "^0.5.5",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.1",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-hot-server-middleware": "^0.6.0",
    "webpack-node-externals": "^1.7.2"
  }
}
